{
  "name": "chainwhisper",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "build": "tsc -p tsconfig.json && tsc-alias",
    "pm2:prod": "pm2 start dist/index.js --name chainwhisper-prod -i max --env production",
    "pm2:restart": "pm2 restart chainwhisper-prod",
    "pm2:stop": "pm2 stop chainwhisper-prod",
    "pm2:logs": "pm2 logs chainwhisper-prod",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write 'src/**/*.ts'"
  },
  "dependencies": {
    "@coinbase/cdp-agentkit-core": "latest",
    "@coinbase/cdp-langchain": "latest",
    "@langchain/core": "^0.3.37",
    "@langchain/langgraph": "latest",
    "@langchain/openai": "^0.4.2",
    "amqplib": "^0.10.5",
    "cors": "^2.8.5",
    "dockerode": "^4.0.4",
    "dotenv": "^16.0.3",
    "express": "^4.21.2",
    "helmet": "^8.0.0",
    "http": "^0.0.1-security",
    "ioredis": "^5.4.2",
    "openai": "^4.82.0",
    "winston": "^3.8.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@types/amqplib": "^0.10.6",
    "@types/cors": "^2.8.17",
    "@types/dockerode": "^3.3.34",
    "@types/express": "^5.0.0",
    "@types/helmet": "^0.0.48",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.37.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "semantic-release": "^24.2.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.3"
  }
}
